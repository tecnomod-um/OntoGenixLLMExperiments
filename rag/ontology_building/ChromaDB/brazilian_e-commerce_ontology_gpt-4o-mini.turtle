@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fo: <https://base_ontology.com#> .

fo:CustomerOntology rdf:type owl:Ontology .

### Classes ###

fo:class_entity rdf:type owl:Class ;
    rdfs:label "Entity" .

fo:customer rdf:type owl:Class ;
    rdfs:subClassOf fo:class_entity ;
    rdfs:label "Customer" .

### Data Properties ###

fo:city_name rdf:type owl:DatatypeProperty ;
    rdfs:domain fo:class_entity ;
    rdfs:range xsd:string ;
    rdfs:label "City Name" .

fo:customer_identifier rdf:type owl:DatatypeProperty ;
    rdfs:domain fo:class_entity ;
    rdfs:range xsd:string ;
    rdfs:label "Customer Identifier" .

fo:state_name rdf:type owl:DatatypeProperty ;
    rdfs:domain fo:class_entity ;
    rdfs:range xsd:string ;
    rdfs:label "State Name" .

fo:unique_customer_identifier rdf:type owl:DatatypeProperty ;
    rdfs:domain fo:class_entity ;
    rdfs:range xsd:string ;
    rdfs:label "Unique Customer Identifier" .

fo:zip_code_prefix_value rdf:type owl:DatatypeProperty ;
    rdfs:domain fo:class_entity ;
    rdfs:range xsd:decimal ;
    rdfs:label "Zip Code Prefix Value" .

### Object Properties ###

fo:resides_in rdf:type owl:ObjectProperty ;
    rdfs:domain fo:class_entity ;
    rdfs:range fo:customer_city ;
    rdfs:label "Resides In" .

fo:identified_by rdf:type owl:ObjectProperty ;
    rdfs:domain fo:class_entity ;
    rdfs:range fo:customer_id ;
    rdfs:label "Identified By" .

fo:located_in_state rdf:type owl:ObjectProperty ;
    rdfs:domain fo:class_entity ;
    rdfs:range fo:customer_state ;
    rdfs:label "Located In State" .

fo:uniquely_identified_by rdf:type owl:ObjectProperty ;
    rdfs:domain fo:class_entity ;
    rdfs:range fo:customer_unique_id ;
    rdfs:label "Uniquely Identified By" .

fo:has_zip_code_prefix rdf:type owl:ObjectProperty ;
    rdfs:domain fo:class_entity ;
    rdfs:range fo:customer_zip_code_prefix ;
    rdfs:label "Has Zip Code Prefix" .
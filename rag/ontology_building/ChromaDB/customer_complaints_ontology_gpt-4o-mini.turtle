@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix um: <https://base_ontology.com#> .

um:ComplaintOntology rdf:type owl:Ontology .

### Classes ###

um:Company rdf:type owl:Class ;
    rdfs:label "Company" .

um:Consumer rdf:type owl:Class ;
    rdfs:label "Consumer" .

um:Complaint rdf:type owl:Class ;
    rdfs:label "Complaint" .

um:Issue rdf:type owl:Class ;
    rdfs:label "Issue" .

um:Product rdf:type owl:Class ;
    rdfs:label "Product" .

um:State rdf:type owl:Class ;
    rdfs:label "State" .

um:ZIPCode rdf:type owl:Class ;
    rdfs:label "ZIP Code" .

um:ComplaintID rdf:type owl:Class ;
    rdfs:label "Complaint ID" .

### Object Properties ###

um:hasCompanyResponse rdf:type owl:ObjectProperty ;
    rdfs:domain um:Company ;
    rdfs:range um:Company ;
    rdfs:label "has company response" .

um:hasConsumerNarrative rdf:type owl:ObjectProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range um:Consumer ;
    rdfs:label "has consumer narrative" .

um:hasIssue rdf:type owl:ObjectProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range um:Issue ;
    rdfs:label "has issue" .

um:hasProduct rdf:type owl:ObjectProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range um:Product ;
    rdfs:label "has product" .

um:hasState rdf:type owl:ObjectProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range um:State ;
    rdfs:label "has state" .

um:hasZIPCode rdf:type owl:ObjectProperty ;
    rdfs:domain um:Consumer ;
    rdfs:range um:ZIPCode ;
    rdfs:label "has ZIP code" .

um:hasComplaintID rdf:type owl:ObjectProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range um:ComplaintID ;
    rdfs:label "has complaint ID" .

### Data Properties ###

um:companyName rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Company ;
    rdfs:range xsd:string ;
    rdfs:label "company name" .

um:complaintDateReceived rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range xsd:string ;
    rdfs:label "complaint date received" .

um:complaintDateSent rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range xsd:string ;
    rdfs:label "complaint date sent" .

um:consumerConsent rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Consumer ;
    rdfs:range xsd:string ;
    rdfs:label "consumer consent provided" .

um:consumerDisputed rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Consumer ;
    rdfs:range xsd:string ;
    rdfs:label "consumer disputed" .

um:timelyResponse rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range xsd:string ;
    rdfs:label "timely response" .

um:submissionMethod rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range xsd:string ;
    rdfs:label "submission method" .

um:tags rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range xsd:string ;
    rdfs:label "tags" .
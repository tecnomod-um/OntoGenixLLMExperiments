@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix um: <https://base_ontology.com#> .

um:ProductSalesOntology rdf:type owl:Ontology .

### Classes ###

um:class_entity rdf:type owl:Class ;
    rdfs:label "Entity Class" .

um:entity_name rdf:type owl:Class ;
    rdfs:subClassOf um:class_entity ;
    rdfs:label "Entity Name" .

### Data Properties ###

um:absolute_Url_idx rdf:type owl:DatatypeProperty ;
    rdfs:domain um:class_entity ;
    rdfs:range xsd:float ;
    rdfs:label "Absolute URL Index" .

um:brand_idx rdf:type owl:DatatypeProperty ;
    rdfs:domain um:class_entity ;
    rdfs:range xsd:float ;
    rdfs:label "Brand Index" .

um:category_idx rdf:type owl:DatatypeProperty ;
    rdfs:domain um:class_entity ;
    rdfs:range xsd:float ;
    rdfs:label "Category Index" .

um:discountPrice_idx rdf:type owl:DatatypeProperty ;
    rdfs:domain um:class_entity ;
    rdfs:range xsd:float ;
    rdfs:label "Discount Price Index" .

um:price_idx rdf:type owl:DatatypeProperty ;
    rdfs:domain um:class_entity ;
    rdfs:range xsd:float ;
    rdfs:label "Price Index" .

um:productName_idx rdf:type owl:DatatypeProperty ;
    rdfs:domain um:class_entity ;
    rdfs:range xsd:float ;
    rdfs:label "Product Name Index" .

um:subCategory_idx rdf:type owl:DatatypeProperty ;
    rdfs:domain um:class_entity ;
    rdfs:range xsd:float ;
    rdfs:label "Subcategory Index" .

um:FAIR_URI rdf:type owl:DatatypeProperty ;

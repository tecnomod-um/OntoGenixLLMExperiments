  <rdf:Description rdf:about="http://oops.linkeddata.es/def#warning">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://oops.linkeddata.es/data/c5dd5f00-27d0-47da-9578-7b2ea6f97848">
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/2a564932-a82a-4598-80a9-f1b360a961f8"/>

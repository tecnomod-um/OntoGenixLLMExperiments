  <rdf:Description rdf:about="http://oops.linkeddata.es/data/6c0fc53c-84f9-4710-b145-077e5a2dd082">
    <oops:hasAffectedElement rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://base_ontology.com#Has_Id</oops:hasAffectedElement>

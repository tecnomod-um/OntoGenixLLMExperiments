  <rdf:Description rdf:about="http://oops.linkeddata.es/data/743305df-a460-4059-bee0-ca81b84e4267">
    <oops:hasAffectedElement rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://base_ontology.com#has_min</oops:hasAffectedElement>
    <oops:hasAffectedElement rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://base_ontology.com#has_mean</oops:hasAffectedElement>

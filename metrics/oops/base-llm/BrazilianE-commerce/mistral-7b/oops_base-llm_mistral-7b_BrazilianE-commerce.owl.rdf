    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/90002763-d918-4ab7-8413-c4da77d8edaa"/>
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/6bb36ed5-2f79-488f-9b5d-df8da2cc3c6c"/>
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/6b1fc9bb-e5ac-40b5-9fb9-112496513ce0"/>

    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/f991f8a2-c91f-47d6-86c6-8cbf7dc48ddd"/>
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/f90ab5ff-6b3c-4347-8afd-2c2feb31729f"/>
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/41e08718-1ee0-4100-817c-674a797f2efd"/>
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/e7b6d4d1-0213-4b8b-9ffe-a25704e41fae"/>

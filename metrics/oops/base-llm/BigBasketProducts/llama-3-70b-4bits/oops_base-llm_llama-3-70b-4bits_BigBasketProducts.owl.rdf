  </rdf:Description>
  <rdf:Description rdf:about="http://oops.linkeddata.es/data/7a866de0-e0ad-44e9-8e12-265912b3ebf0">
    <oops:hasAffectedElement rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://base_ontology.com#hasBrand</oops:hasAffectedElement>
    <oops:hasAffectedElement rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://base_ontology.com#hasCategory</oops:hasAffectedElement>
    <oops:hasAffectedElement rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://base_ontology.com#hasImage</oops:hasAffectedElement>

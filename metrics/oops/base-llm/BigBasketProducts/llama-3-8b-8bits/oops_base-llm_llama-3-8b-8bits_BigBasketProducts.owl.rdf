  <rdf:Description rdf:about="http://oops.linkeddata.es/data/017a7293-fe65-4f1d-aee3-a523cc70dd90">
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/6347a039-b3e9-4d6d-8f66-5a3800abd1d5"/>
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/637d2ba5-5941-4723-8666-66bf7e82dc5d"/>

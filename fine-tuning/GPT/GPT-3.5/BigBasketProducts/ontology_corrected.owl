@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix um: <https://vocab.um.es/> .

<https://vocab.um.es/CommercialActivities#> rdf:type owl:Ontology .

### Classes ###

### Class Declarations ###
um:Product a owl:Class ;
    rdfs:label "Product"@en ;
    rdfs:comment "Represents a product in a commercial context"@en .

um:Brand a owl:Class ;
    rdfs:label "Brand"@en ;
    rdfs:comment "Represents a brand of a product in a commercial context"@en .


### Data Properties ###

### Data Property Declarations ###
um:productName a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "product name"@en ;
    rdfs:range xsd:string ;
    rdfs:comment "The name of the product"@en .

um:price a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "price"@en ;
    rdfs:range xsd:decimal ;
    rdfs:comment "The price of the product"@en .

um:discountPrice a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "discount price"@en ;
    rdfs:range xsd:decimal ;
    rdfs:comment "The discounted price of the product"@en .

um:quantity a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "quantity"@en ;
    rdfs:range xsd:string ;
    rdfs:comment "The quantity of the product"@en .


### Object Properties ###

### Object Property Declarations ###
um:isProducedBy a owl:ObjectProperty ;
    rdfs:domain um:Product ;
    rdfs:range um:Brand ;
    rdfs:label "is produced by"@en ;
    rdfs:comment "Relates a product to the brand producing it"@en .


### Named Individuals ###

### Dataset Attributes (with some examples) ###
um:productNameList a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "product name list"@en ;
    rdfs:range xsd:string ;
    rdfs:comment "The name of the products in the dataset"@en .

um:brandList a owl:DatatypeProperty ;
    rdfs:domain um:Brand ;
    rdfs:label "brand list"@en ;
    rdfs:range xsd:string ;
    rdfs:comment "The names of the brands in the dataset"@en .

um:minPrice a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "minimum price"@en ;
    rdfs:range xsd:decimal ;
    rdfs:comment "The minimum price of the products in the dataset"@en .

um:maxPrice a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "maximum price"@en ;
    rdfs:range xsd:decimal ;
    rdfs:comment "The maximum price of the products in the dataset"@en .

um:minDiscountPrice a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "minimum discount price"@en ;
    rdfs:range xsd:decimal ;
    rdfs:comment "The minimum discounted price of the products in the dataset"@en .

um:maxDiscountPrice a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "maximum discount price"@en ;
    rdfs:range xsd:decimal ;
    rdfs:comment "The maximum discounted price of the products in the dataset"@en .

um:imageUrl a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "image URL"@en ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "The URL of the image of the products"@en .

um:category a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "category"@en ;
    rdfs:range xsd:string ;
    rdfs:comment "The category of the products"@en .

um:subCategory a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "sub-category"@en ;
    rdfs:range xsd:string ;
    rdfs:comment "The sub-category of the products"@en .

um:absoluteUrl a owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:label "absolute URL"@en ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "The absolute URL of the product"@en .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix um: <https://vocab.um.es#> .

um:CommercialOntology rdf:type owl:Ontology .

### Classes ###

um:Invoice rdf:type owl:Class ;
    rdfs:label "Invoice"@en .

um:Product rdf:type owl:Class ;
    rdfs:label "Product"@en .

um:Customer rdf:type owl:Class ;
    rdfs:label "Customer"@en .

um:Country rdf:type owl:Class ;
    rdfs:label "Country"@en .

### Data Properties ###

um:invoiceDate rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Invoice ;
    rdfs:range xsd:dateTime ;
    rdfs:label "invoice date"@en .

um:stockCode rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:range xsd:string ;
    rdfs:label "stock code"@en .

um:description rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:range xsd:string ;
    rdfs:label "description"@en .

um:quantity rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Invoice ;
    rdfs:range xsd:integer ;
    rdfs:label "quantity"@en .

um:unitPrice rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:range xsd:decimal ;
    rdfs:label "unit price"@en .

um:customerID rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Customer ;
    rdfs:range xsd:integer ;
    rdfs:label "customer ID"@en .

### Object Properties ###

um:hasProduct rdf:type owl:ObjectProperty ;
    rdfs:domain um:Invoice ;
    rdfs:range um:Product ;
    rdfs:label "has product"@en .

um:belongsToCustomer rdf:type owl:ObjectProperty ;
    rdfs:domain um:Invoice ;
    rdfs:range um:Customer ;
    rdfs:label "belongs to customer"@en .

um:locatedInCountry rdf:type owl:ObjectProperty ;
    rdfs:domain um:Customer ;
    rdfs:range um:Country ;
    rdfs:label "located in country"@en .

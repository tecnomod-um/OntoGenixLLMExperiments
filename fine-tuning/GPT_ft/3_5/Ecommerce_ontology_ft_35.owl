@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
    
@prefix owl: <http://www.w3.org/2002/07/owl#> .
    
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    
@prefix um: <https://vocab.um.es#> .
    
    
um:SalesOntology rdf:type owl:Ontology .
    
    
### Classes
    
um:SalesInvoice
    rdf:type owl:Class ;
    rdfs:label "Sales Invoice" .
    
um:StockCode
    rdf:type owl:Class ;
    rdfs:label "Stock Code" .
    
um:Description
    rdf:type owl:Class ;
    rdfs:label "Description" .
    
um:Quantity
    rdf:type owl:Class ;
    rdfs:label "Quantity" .
    
um:InvoiceDate
    rdf:type owl:Class ;
    rdfs:label "Invoice Date" .
    
um:UnitPrice
    rdf:type owl:Class ;
    rdfs:label "Unit Price" .
    
um:CustomerID
    rdf:type owl:Class ;
    rdfs:label "Customer ID" .
    
um:Country
    rdf:type owl:Class ;
    rdfs:label "Country" .
    
    
### Data Properties
    
um:hasInvoiceNumber
    rdf:type owl:DatatypeProperty ;
    rdfs:label "has Invoice Number" ;
    rdfs:domain um:SalesInvoice ;
    rdfs:range xsd:string .
    
um:hasStockCode
    rdf:type owl:DatatypeProperty ;
    rdfs:label "has Stock Code" ;
    rdfs:domain um:StockCode ;
    rdfs:range xsd:string .
    
um:hasDescription
    rdf:type owl:DatatypeProperty ;
    rdfs:label "has Description" ;
    rdfs:domain um:Description ;
    rdfs:range xsd:string .
    
um:hasQuantity
    rdf:type owl:DatatypeProperty ;
    rdfs:label "has Quantity" ;
    rdfs:domain um:Quantity ;
    rdfs:range xsd:integer .
    
um:hasInvoiceDate
    rdf:type owl:DatatypeProperty ;
    rdfs:label "has Invoice Date" ;
    rdfs:domain um:InvoiceDate ;
    rdfs:range xsd:string .
    
um:hasUnitPrice
    rdf:type owl:DatatypeProperty ;
    rdfs:label "has Unit Price" ;
    rdfs:domain um:UnitPrice ;
    rdfs:range xsd:decimal .
    
um:hasCustomerID
    rdf:type owl:DatatypeProperty ;
    rdfs:label "has Customer ID" ;
    rdfs:domain um:CustomerID ;
    rdfs:range xsd:decimal .
    
um:hasCountry
    rdf:type owl:DatatypeProperty ;
    rdfs:label "has Country" ;
    rdfs:domain um:Country ;
    rdfs:range xsd:string .
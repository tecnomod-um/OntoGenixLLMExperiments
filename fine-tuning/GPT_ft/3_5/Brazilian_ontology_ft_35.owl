@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix um: <https://vocab.um.es#> .

um:Customers  a owl:Class .

um:CustomerID  a owl:DatatypeProperty;
    rdfs:domain um:Customers;
    rdfs:range xsd:string .

um:CustomerUniqueID  a owl:DatatypeProperty;
    rdfs:domain um:Customers;
    rdfs:range xsd:string .

um:CustomerZipCodePrefix  a owl:DatatypeProperty;
    rdfs:domain um:Customers;
    rdfs:range xsd:integer .

um:CustomerCity  a owl:DatatypeProperty;
    rdfs:domain um:Customers;
    rdfs:range xsd:string .

um:CustomerState  a owl:DatatypeProperty;
    rdfs:domain um:Customers;
    rdfs:range xsd:string .


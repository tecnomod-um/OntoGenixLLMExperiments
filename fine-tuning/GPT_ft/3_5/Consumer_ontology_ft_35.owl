@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix um: <https://vocab.um.es#> .

um:UserComplaintsOntology rdf:type owl:Ontology .

### Classes and Subclasses

### Main Class
um:Complaint rdf:type owl:Class ;
    rdfs:label "Complaint" .

### Subclasses
um:Product rdf:type owl:Class ;
    rdfs:label "Product" .

um:SubProduct rdf:type owl:Class ;
    rdfs:label "Sub-product" .

um:Issue rdf:type owl:Class ;
    rdfs:label "Issue" .

um:SubIssue rdf:type owl:Class ;
    rdfs:label "Sub-issue" .

um:Company rdf:type owl:Class ;
    rdfs:label "Company" .


### Data Properties

### Attributes for Complaint Class
um:dateReceived rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range xsd:date ;
    rdfs:label "Date received" .

um:state rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range xsd:string ;
    rdfs:label "State" .

### Attributes for Product Class
um:productName rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Product ;
    rdfs:range xsd:string ;
    rdfs:label "Product name" .

um:subProductName rdf:type owl:DatatypeProperty ;
    rdfs:domain um:SubProduct ;
    rdfs:range xsd:string ;
    rdfs:label "Sub-product name" .

### Attributes for Issue Class
um:issueType rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Issue ;
    rdfs:range xsd:string ;
    rdfs:label "Issue type" .

um:consumerComplaintNarrative rdf:type owl:DatatypeProperty ;
    rdfs:domain um:Issue ;
    rdfs:range xsd:string ;
    rdfs:label "Consumer complaint narrative" .

### Attributes for SubIssue Class
um:subIssueType rdf:type owl:DatatypeProperty ;
    rdfs:domain um:SubIssue ;
    rdfs:range xsd:string ;
    rdfs:label "Sub-issue type" .


### Relation between classes and subclasses
um:hasProduct rdf:type owl:ObjectProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range um:Product ;
    rdfs:label "Has product" .

um:hasSubProduct rdf:type owl:ObjectProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range um:SubProduct ;
    rdfs:label "Has sub-product" .

um:hasIssue rdf:type owl:ObjectProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range um:Issue ;
    rdfs:label "Has issue" .

um:hasSubIssue rdf:type owl:ObjectProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range um:SubIssue ;
    rdfs:label "Has sub-issue" .

um:relatedCompany rdf:type owl:ObjectProperty ;
    rdfs:domain um:Complaint ;
    rdfs:range um:Company ;
    rdfs:label "Related company" .
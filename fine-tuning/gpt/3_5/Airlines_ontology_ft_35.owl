@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix um: <https://vocab.um.es#> .

um:CommercialActivityRatings rdf:type owl:Ontology .

### Classes
um:Customer rdf:type owl:Class ;
    rdfs:label "Customer"@en .

um:Flight rdf:type owl:Class ;
    rdfs:label "Flight"@en .

um:CustomerRating rdf:type owl:Class ;
    rdfs:label "Customer Rating"@en .

### Datatype Properties
um:hasAttribute rdf:type owl:DatatypeProperty ;
    rdfs:domain um:CustomerRating ;
    rdfs:range xsd:string ;
    rdfs:label "has Attribute"@en .

um:hasNumericValue rdf:type owl:DatatypeProperty ;
    rdfs:domain um:CustomerRating ;
    rdfs:range xsd:decimal ;
    rdfs:label "has Numeric Value"@en .

### Object Properties
um:hasCustomer rdf:type owl:ObjectProperty ;
    rdfs:domain um:CustomerRating ;
    rdfs:range um:Customer ;
    rdfs:label "has Customer"@en .

um:hasFlight rdf:type owl:ObjectProperty ;
    rdfs:domain um:Customer ;
    rdfs:range um:Flight ;
    rdfs:label "has Flight"@en .

um:ratedBy rdf:type owl:ObjectProperty ;
    rdfs:domain um:Customer ;
    rdfs:range um:CustomerRating ;
    rdfs:label "rated By"@en .

### Individuals
um:LoyalCustomer rdf:type owl:NamedIndividual ;
    rdf:type um:Customer ;
    rdfs:label "Loyal Customer"@en .

um:DisloyalCustomer rdf:type owl:NamedIndividual ;
    rdf:type um:Customer ;
    rdfs:label "Disloyal Customer"@en .

um:BusinessTravel rdf:type owl:NamedIndividual ;
    rdf:type um:Flight ;
    rdfs:label "Business Travel"@en .

um:PersonalTravel rdf:type owl:NamedIndividual ;
    rdf:type um:Flight ;
    rdfs:label "Personal Travel"@en .

um:BusinessClass rdf:type owl:NamedIndividual ;
    rdf:type um:Flight ;
    rdfs:label "Business Class"@en .

um:EcoClass rdf:type owl:NamedIndividual ;
    rdf:type um:Flight ;
    rdfs:label "Eco Class"@en .

um:EcoPlusClass rdf:type owl:NamedIndividual ;
    rdf:type um:Flight ;
    rdfs:label "Eco Plus Class"@en .
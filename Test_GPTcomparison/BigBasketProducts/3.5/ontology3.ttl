@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

base:Absolute_Url a owl:Class .
base:Brand a owl:Class .
base:Category a owl:Class .
base:Image_Url a owl:Class .
base:ProductName a owl:Class .
base:Quantity a owl:Class .
base:SubCategory a owl:Class .
base:DiscountPrice a owl:Class .
base:Price a owl:Class .
base:ExternalResource a owl:Class .

base:hasNonNullCount a owl:ObjectProperty ;
    rdfs:domain base:Absolute_Url, base:Brand, base:Category, base:Image_Url, base:ProductName, base:Quantity, base:SubCategory, base:DiscountPrice, base:Price ;
    rdfs:range xsd:float .

base:hasType a owl:ObjectProperty ;
    rdfs:domain base:Absolute_Url, base:Brand, base:Category, base:Image_Url, base:ProductName, base:Quantity, base:SubCategory, base:DiscountPrice, base:Price ;
    rdfs:range xsd:string .

base:hasUniqueCount a owl:ObjectProperty ;
    rdfs:domain base:Absolute_Url, base:Brand, base:Category, base:Image_Url, base:ProductName, base:Quantity, base:SubCategory, base:DiscountPrice, base:Price ;
    rdfs:range xsd:float .

base:hasCount a owl:ObjectProperty ;
    rdfs:domain base:Absolute_Url, base:Brand, base:Category, base:Image_Url, base:ProductName, base:Quantity, base:SubCategory, base:DiscountPrice, base:Price ;
    rdfs:range xsd:float .

base:hasMax a owl:ObjectProperty ;
    rdfs:domain base:DiscountPrice, base:Price ;
    rdfs:range xsd:float .

base:hasMean a owl:ObjectProperty ;
    rdfs:domain base:DiscountPrice, base:Price ;
    rdfs:range xsd:float .

base:hasMin a owl:ObjectProperty ;
    rdfs:domain base:DiscountPrice, base:Price ;
    rdfs:range xsd:float .

base:hasStd a owl:ObjectProperty ;
    rdfs:domain base:DiscountPrice, base:Price ;
    rdfs:range xsd:float .

base:linksToExternalResource a owl:ObjectProperty ;
    rdfs:domain base:Absolute_Url, base:Brand, base:Category, base:Image_Url, base:ProductName, base:Quantity, base:SubCategory, base:DiscountPrice, base:Price ;
    rdfs:range base:ExternalResource .
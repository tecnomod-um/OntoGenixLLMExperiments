@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix gr: <http://purl.org/goodrelations/v1#> .

base:Country rdf:type owl:Class .
base:Description rdf:type owl:Class .
base:Invoice rdf:type owl:Class .
base:StockItem rdf:type owl:Class .
base:Customer rdf:type owl:Class .
base:QuantitativeValue rdf:type owl:Class .
base:PriceSpecification rdf:type owl:Class .

base:hasCountry rdf:type owl:ObjectProperty ;
    rdfs:domain base:Invoice ;
    rdfs:range base:Country .

base:hasDescription rdf:type owl:ObjectProperty ;
    rdfs:domain base:StockItem ;
    rdfs:range base:Description .

base:hasInvoiceDate rdf:type owl:DatatypeProperty ;
    rdfs:domain base:Invoice ;
    rdfs:range xsd:string .

base:hasInvoiceNo rdf:type owl:DatatypeProperty ;
    rdfs:domain base:Invoice ;
    rdfs:range xsd:string .

base:hasStockCode rdf:type owl:DatatypeProperty ;
    rdfs:domain base:StockItem ;
    rdfs:range xsd:string .

base:hasCustomerID rdf:type owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:integer .

base:hasQuantity rdf:type owl:DatatypeProperty ;
    rdfs:domain base:QuantitativeValue ;
    rdfs:range xsd:integer .

base:hasUnitPrice rdf:type owl:DatatypeProperty ;
    rdfs:domain base:PriceSpecification ;
    rdfs:range xsd:decimal .

base:hasStockItem rdf:type owl:ObjectProperty ;
    rdfs:domain base:Invoice ;
    rdfs:range base:StockItem .

base:hasCustomer rdf:type owl:ObjectProperty ;
    rdfs:domain base:Invoice ;
    rdfs:range base:Customer .

base:hasQuantitativeValue rdf:type owl:ObjectProperty ;
    rdfs:domain base:StockItem ;
    rdfs:range base:QuantitativeValue .

base:hasPriceSpecification rdf:type owl:ObjectProperty ;
    rdfs:domain base:StockItem ;
    rdfs:range base:PriceSpecification .

base:Country owl:equivalentClass <http://dbpedia.org/resource/Country> .
base:Description owl:equivalentClass gr:description .
base:InvoiceDate owl:equivalentClass dcterms:date .
base:InvoiceNo owl:equivalentClass gr:BusinessEntity .
base:StockCode owl:equivalentClass gr:hasStockKeepingUnit .
base:CustomerID owl:equivalentClass foaf:Person .
base:Quantity owl:equivalentClass gr:quantitativeProductOrServiceProperty .
base:UnitPrice owl:equivalentClass gr:hasPriceSpecification .
@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix gr: <http://purl.org/goodrelations/v1#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

base:Transaction a owl:Class ;
    rdfs:subClassOf schema:Order .

base:Product a owl:Class ;
    rdfs:subClassOf schema:Product .

base:Customer a owl:Class ;
    rdfs:subClassOf schema:Person, schema:Organization .

base:Invoice a owl:Class ;
    rdfs:subClassOf schema:Invoice .

base:InvoiceDate a owl:Class ;
    rdfs:subClassOf base:Invoice ;
    owl:equivalentClass schema:DateTime .

base:InvoiceNo a owl:Class ;
    rdfs:subClassOf base:Invoice ;
    owl:equivalentClass schema:identifier .

base:StockCode a owl:Class ;
    rdfs:subClassOf base:Product ;
    owl:equivalentClass schema:ProductModel, schema:sku .

base:Description a owl:Class ;
    rdfs:subClassOf base:Product ;
    owl:equivalentClass schema:description .

base:CustomerID a owl:Class ;
    rdfs:subClassOf base:Customer ;
    owl:equivalentClass schema:identifier .

base:Country a owl:Class ;
    rdfs:subClassOf base:Transaction ;
    owl:equivalentClass schema:Country .

base:hasProduct a owl:ObjectProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range base:Product .

base:hasCustomer a owl:ObjectProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range base:Customer .

base:hasInvoice a owl:ObjectProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range base:Invoice .

base:locatedIn a owl:ObjectProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range base:Country .

base:countryName a owl:DatatypeProperty ;
    rdfs:domain base:Country ;
    rdfs:range xsd:string .

base:descriptionText a owl:DatatypeProperty ;
    rdfs:domain base:Description ;
    rdfs:range xsd:string .

base:invoiceDateValue a owl:DatatypeProperty ;
    rdfs:domain base:InvoiceDate ;
    rdfs:range xsd:string .

base:invoiceNumber a owl:DatatypeProperty ;
    rdfs:domain base:InvoiceNo ;
    rdfs:range xsd:string .

base:stockCodeValue a owl:DatatypeProperty ;
    rdfs:domain base:StockCode ;
    rdfs:range xsd:string .

base:customerIdentifier a owl:DatatypeProperty ;
    rdfs:domain base:CustomerID ;
    rdfs:range xsd:integer .

base:quantityValue a owl:DatatypeProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range xsd:integer .

base:unitPriceValue a owl:DatatypeProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range xsd:decimal .
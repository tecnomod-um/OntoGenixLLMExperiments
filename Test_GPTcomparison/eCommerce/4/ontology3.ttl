@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .
@prefix gr: <http://purl.org/goodrelations/v1#> .

base:Transaction rdf:type owl:Class ;
    rdfs:subClassOf schema:Order .

base:Product rdf:type owl:Class ;
    rdfs:subClassOf schema:Product .

base:Customer rdf:type owl:Class ;
    rdfs:subClassOf foaf:Person .

base:Invoice rdf:type owl:Class ;
    rdfs:subClassOf schema:Invoice .

base:Country rdf:type owl:Class ;
    rdfs:subClassOf dbo:Country .

base:hasProduct rdf:type owl:ObjectProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range base:Product .

base:hasCustomer rdf:type owl:ObjectProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range base:Customer .

base:hasInvoice rdf:type owl:ObjectProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range base:Invoice .

base:locatedIn rdf:type owl:ObjectProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range base:Country .

base:description rdf:type owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range xsd:string .

base:invoiceDate rdf:type owl:DatatypeProperty ;
    rdfs:domain base:Invoice ;
    rdfs:range xsd:dateTime .

base:invoiceNo rdf:type owl:DatatypeProperty ;
    rdfs:domain base:Invoice ;
    rdfs:range xsd:string .

base:stockCode rdf:type owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range xsd:string .

base:customerID rdf:type owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:integer .

base:quantity rdf:type owl:DatatypeProperty ;
    rdfs:domain base:Transaction ;
    rdfs:range xsd:integer .

base:unitPrice rdf:type owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range xsd:decimal .
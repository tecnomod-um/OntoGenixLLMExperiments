@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

base:Country a owl:Class ;
    rdfs:subClassOf schema:Country .

base:Description a owl:Class ;
    rdfs:subClassOf dcterms:description .

base:InvoiceDate a owl:Class ;
    rdfs:subClassOf schema:Date .

base:InvoiceNo a owl:Class ;
    rdfs:subClassOf schema:identifier .

base:StockCode a owl:Class ;
    rdfs:subClassOf schema:Product .

base:CustomerID a owl:Class ;
    rdfs:subClassOf foaf:Person .

base:Quantity a owl:Class ;
    rdfs:subClassOf schema:QuantitativeValue .

base:UnitPrice a owl:Class ;
    rdfs:subClassOf schema:UnitPriceSpecification .

base:hasCountry a owl:ObjectProperty ;
    rdfs:domain base:InvoiceNo ;
    rdfs:range base:Country .

base:hasDescription a owl:ObjectProperty ;
    rdfs:domain base:StockCode ;
    rdfs:range base:Description .

base:hasInvoiceDate a owl:ObjectProperty ;
    rdfs:domain base:InvoiceNo ;
    rdfs:range base:InvoiceDate .

base:hasInvoiceNo a owl:ObjectProperty ;
    rdfs:domain base:StockCode ;
    rdfs:range base:InvoiceNo .

base:hasStockCode a owl:ObjectProperty ;
    rdfs:domain base:InvoiceNo ;
    rdfs:range base:StockCode .

base:hasCustomerID a owl:ObjectProperty ;
    rdfs:domain base:InvoiceNo ;
    rdfs:range base:CustomerID .

base:hasQuantity a owl:ObjectProperty ;
    rdfs:domain base:StockCode ;
    rdfs:range base:Quantity .

base:hasUnitPrice a owl:ObjectProperty ;
    rdfs:domain base:StockCode ;
    rdfs:range base:UnitPrice .
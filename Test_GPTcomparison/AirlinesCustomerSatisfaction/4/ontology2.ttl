@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix schema: <http://schema.org/> .

base:Passenger rdf:type owl:Class ;
    rdfs:subClassOf foaf:Person .

base:Flight rdf:type owl:Class ;
    rdfs:subClassOf schema:Flight .

base:ServiceRating rdf:type owl:Class ;
    rdfs:subClassOf schema:Rating .

base:Travel rdf:type owl:Class ;
    rdfs:subClassOf schema:TravelAction .

base:NumericalAttribute rdf:type owl:Class ;
    rdfs:subClassOf dbo:Number .

base:CategoricalAttribute rdf:type owl:Class ;
    rdfs:subClassOf schema:QualitativeValue .

base:FlightAttribute rdf:type owl:Class ;
    rdfs:subClassOf schema:flight .

base:ServiceRatingAttribute rdf:type owl:Class ;
    rdfs:subClassOf schema:ratingValue .

base:TravelAttribute rdf:type owl:Class ;
    rdfs:subClassOf schema:TravelAction .

base:age rdf:type owl:ObjectProperty ;
    rdfs:domain base:Passenger ;
    rdfs:range base:NumericalAttribute .

base:arrivalDelay rdf:type owl:ObjectProperty ;
    rdfs:domain base:Flight ;
    rdfs:range base:NumericalAttribute .

base:departureDelay rdf:type owl:ObjectProperty ;
    rdfs:domain base:Flight ;
    rdfs:range base:NumericalAttribute .

base:flightDistance rdf:type owl:ObjectProperty ;
    rdfs:domain base:Flight ;
    rdfs:range base:NumericalAttribute .

base:baggageHandling rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:checkinService rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:cleanliness rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:departureArrivalTimeConvenience rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:easeOfOnlineBooking rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:foodAndDrink rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:gateLocation rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:inflightEntertainment rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:inflightWifiService rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:legRoomService rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:onBoardService rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:onlineBoarding rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:onlineSupport rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:seatComfort rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .

base:travelClass rdf:type owl:ObjectProperty ;
    rdfs:domain base:Flight ;
    rdfs:range base:CategoricalAttribute .

base:customerType rdf:type owl:ObjectProperty ;
    rdfs:domain base:Passenger ;
    rdfs:range base:CategoricalAttribute .

base:gender rdf:type owl:ObjectProperty ;
    rdfs:domain base:Passenger ;
    rdfs:range base:CategoricalAttribute .

base:typeOfTravel rdf:type owl:ObjectProperty ;
    rdfs:domain base:Travel ;
    rdfs:range base:CategoricalAttribute .

base:satisfaction rdf:type owl:ObjectProperty ;
    rdfs:domain base:ServiceRating ;
    rdfs:range base:CategoricalAttribute .
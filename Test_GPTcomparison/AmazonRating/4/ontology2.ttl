@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <http://schema.org/> .
@prefix gr: <http://purl.org/goodrelations/v1#> .
@prefix sio: <http://semanticscience.org/resource/> .

base:productId rdf:type owl:DatatypeProperty ;
    rdfs:domain gr:ProductOrService ;
    rdfs:range xsd:string .

base:ratingValue rdf:type owl:DatatypeProperty ;
    rdfs:domain schema:Review ;
    rdfs:range xsd:integer .

base:timestampValue rdf:type owl:DatatypeProperty ;
    rdfs:domain schema:Review ;
    rdfs:range xsd:dateTime .

base:userId rdf:type owl:DatatypeProperty ;
    rdfs:domain foaf:Person ;
    rdfs:range xsd:string .

base:hasReview rdf:type owl:ObjectProperty ;
    rdfs:domain foaf:Person ;
    rdfs:range schema:Review .

base:reviewedProduct rdf:type owl:ObjectProperty ;
    rdfs:domain schema:Review ;
    rdfs:range gr:ProductOrService .

base:reviewer rdf:type owl:ObjectProperty ;
    rdfs:domain schema:Review ;
    rdfs:range foaf:Person .
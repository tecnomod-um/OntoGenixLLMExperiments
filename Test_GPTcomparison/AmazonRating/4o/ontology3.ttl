@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

base:Product a owl:Class ;
    rdfs:subClassOf schema:Product .

base:User a owl:Class ;
    rdfs:subClassOf foaf:Person .

base:RatingEvent a owl:Class ;
    rdfs:subClassOf schema:Review .

base:ProductId a owl:Class ;
    rdfs:subClassOf base:Product .

base:UserId a owl:Class ;
    rdfs:subClassOf base:User .

base:Rating a owl:Class ;
    rdfs:subClassOf base:RatingEvent .

base:Timestamp a owl:Class ;
    rdfs:subClassOf base:RatingEvent .

base:hasRating a owl:ObjectProperty ;
    rdfs:domain base:Product ;
    rdfs:range base:RatingEvent .

base:ratedBy a owl:ObjectProperty ;
    rdfs:domain base:RatingEvent ;
    rdfs:range base:User .

base:productId a owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range rdfs:Literal .

base:userId a owl:DatatypeProperty ;
    rdfs:domain base:User ;
    rdfs:range rdfs:Literal .

base:ratingValue a owl:DatatypeProperty ;
    rdfs:domain base:RatingEvent ;
    rdfs:range rdfs:Literal .

base:ratingTimestamp a owl:DatatypeProperty ;
    rdfs:domain base:RatingEvent ;
    rdfs:range rdfs:Literal .
  </rdf:Description>
  <rdf:Description rdf:about="http://oops.linkeddata.es/data/9f033ddc-8cb9-4919-be62-73dae36ccaab">
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/8764787f-a262-4914-8848-444a22b5b127"/>
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/19da5a8a-37c5-4981-b29b-521bdcb6b1a5"/>
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/15de3f13-3898-47f7-8f2b-892b0c530a05"/>
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/74c49746-d1e7-47d6-8650-deb405cecee1"/>
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/72cc63cc-4aa3-4c4f-a9c1-929a87d89849"/>

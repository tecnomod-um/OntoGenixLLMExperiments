@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.com/> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:class_entity a owl:Class .

ex:Rating rdfs:subClassOf ex:class_entity .
ex:Timestamp rdfs:subClassOf ex:class_entity .
ex:UserId rdfs:subClassOf ex:class_entity .
ex:ProductId rdfs:subClassOf ex:class_entity .

ex:hasRating a owl:ObjectProperty ;
    rdfs:domain ex:class_entity ;
    rdfs:range ex:Rating .

ex:hasTimestamp a owl:ObjectProperty ;
    rdfs:domain ex:class_entity ;
    rdfs:range ex:Timestamp .

ex:hasUserId a owl:ObjectProperty ;
    rdfs:domain ex:class_entity ;
    rdfs:range ex:UserId .

ex:hasProductId a owl:DatatypeProperty ;
    rdfs:domain ex:class_entity ;
    rdfs:range xsd:string .

ex:Rating rdfs:subClassOf schema:Rating .
ex:Timestamp rdfs:subClassOf schema:DateTime .
ex:UserId rdfs:subClassOf schema:identifier .
ex:ProductId rdfs:subClassOf schema:Product .
@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .
@prefix gr: <http://purl.org/goodrelations/v1#> .

base:Company rdf:type owl:Class ;
              rdfs:subClassOf gr:BusinessEntity .

base:PublicResponse rdf:type owl:Class ;
                    rdfs:subClassOf schema:Organization .

base:Communication rdf:type owl:Class ;
                   rdfs:subClassOf schema:Communication .

base:ConsumerNarrative rdf:type owl:Class ;
                       rdfs:subClassOf schema:CreativeWork .

base:ConsumerConsent rdf:type owl:Class ;
                     rdfs:subClassOf dcterms:Consent .

base:Dispute rdf:type owl:Class ;
             rdfs:subClassOf schema:Dispute .

base:Complaint rdf:type owl:Class ;
               rdfs:subClassOf schema:ConsumerComplaint .

base:Issue rdf:type owl:Class ;
           rdfs:subClassOf skos:Concept .

base:Product rdf:type owl:Class ;
             rdfs:subClassOf gr:ProductOrService .

base:Location rdf:type owl:Class ;
              rdfs:subClassOf schema:Place .

base:SubIssue rdf:type owl:Class ;
              rdfs:subClassOf skos:Concept .

base:SubProduct rdf:type owl:Class ;
                rdfs:subClassOf gr:SomeProducts .

base:SubmissionMethod rdf:type owl:Class ;
                      rdfs:subClassOf schema:DigitalDocument .

base:Tag rdf:type owl:Class ;
          rdfs:subClassOf schema:Thing .

base:TimelyResponse rdf:type owl:Class ;
                    rdfs:subClassOf schema:ReplyAction .

base:hasPublicResponse rdf:type owl:ObjectProperty ;
                       rdfs:domain base:Company ;
                       rdfs:range base:PublicResponse .

base:hasCommunication rdf:type owl:ObjectProperty ;
                      rdfs:domain base:Complaint ;
                      rdfs:range base:Communication .

base:hasConsumerNarrative rdf:type owl:ObjectProperty ;
                          rdfs:domain base:Complaint ;
                          rdfs:range base:ConsumerNarrative .

base:hasConsumerConsent rdf:type owl:ObjectProperty ;
                        rdfs:domain base:Complaint ;
                        rdfs:range base:ConsumerConsent .

base:hasDispute rdf:type owl:ObjectProperty ;
                rdfs:domain base:Complaint ;
                rdfs:range base:Dispute .

base:hasIssue rdf:type owl:ObjectProperty ;
              rdfs:domain base:Complaint ;
              rdfs:range base:Issue .

base:hasProduct rdf:type owl:ObjectProperty ;
                rdfs:domain base:Complaint ;
                rdfs:range base:Product .

base:hasLocation rdf:type owl:ObjectProperty ;
                 rdfs:domain base:Complaint ;
                 rdfs:range base:Location .

base:hasSubIssue rdf:type owl:ObjectProperty ;
                 rdfs:domain base:Issue ;
                 rdfs:range base:SubIssue .

base:hasSubProduct rdf:type owl:ObjectProperty ;
                   rdfs:domain base:Product ;
                   rdfs:range base:SubProduct .

base:hasSubmissionMethod rdf:type owl:ObjectProperty ;
                         rdfs:domain base:Complaint ;
                         rdfs:range base:SubmissionMethod .

base:hasTag rdf:type owl:ObjectProperty ;
            rdfs:domain base:Complaint ;
            rdfs:range base:Tag .

base:hasTimelyResponse rdf:type owl:ObjectProperty ;
                       rdfs:domain base:Complaint ;
                       rdfs:range base:TimelyResponse .

base:hasComplaintID rdf:type owl:DatatypeProperty ;
                    rdfs:domain base:Complaint ;
                    rdfs:range xsd:integer .

base:hasZIPCode rdf:type owl:DatatypeProperty ;
                rdfs:domain base:Location ;
                rdfs:range xsd:integer .

base:hasNonNullCount rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:float .

base:hasUniqueCount rdf:type owl:DatatypeProperty ;
                    rdfs:range xsd:float .

base:hasCount rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:float .

base:hasMax rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:float .

base:hasMean rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:float .

base:hasMin rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:float .

base:hasStd rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:float .
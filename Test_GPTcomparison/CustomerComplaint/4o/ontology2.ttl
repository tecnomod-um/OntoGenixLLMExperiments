@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .

base:Complaint a owl:Class .

base:Company a owl:Class ;
    rdfs:subClassOf base:Complaint .

base:Consumer a owl:Class ;
    rdfs:subClassOf base:Complaint .

base:Product a owl:Class ;
    rdfs:subClassOf base:Complaint .

base:Response a owl:Class ;
    rdfs:subClassOf base:Complaint .

base:hasCompany a owl:ObjectProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range base:Company .

base:hasConsumer a owl:ObjectProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range base:Consumer .

base:hasProduct a owl:ObjectProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range base:Product .

base:hasResponse a owl:ObjectProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range base:Response .

base:companyName a owl:DatatypeProperty ;
    rdfs:domain base:Company ;
    rdfs:range xsd:string .

base:companyPublicResponse a owl:DatatypeProperty ;
    rdfs:domain base:Response ;
    rdfs:range xsd:string .

base:companyResponseToConsumer a owl:DatatypeProperty ;
    rdfs:domain base:Response ;
    rdfs:range xsd:string .

base:consumerComplaintNarrative a owl:DatatypeProperty ;
    rdfs:domain base:Consumer ;
    rdfs:range xsd:string .

base:consumerConsentProvided a owl:DatatypeProperty ;
    rdfs:domain base:Consumer ;
    rdfs:range xsd:string .

base:consumerDisputed a owl:DatatypeProperty ;
    rdfs:domain base:Consumer ;
    rdfs:range xsd:string .

base:dateReceived a owl:DatatypeProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range xsd:string .

base:dateSentToCompany a owl:DatatypeProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range xsd:string .

base:issue a owl:DatatypeProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range xsd:string .

base:productType a owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range xsd:string .

base:state a owl:DatatypeProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range xsd:string .

base:subIssue a owl:DatatypeProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range xsd:string .

base:subProduct a owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range xsd:string .

base:submittedVia a owl:DatatypeProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range xsd:string .

base:tags a owl:DatatypeProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range xsd:string .

base:timelyResponse a owl:DatatypeProperty ;
    rdfs:domain base:Response ;
    rdfs:range xsd:string .

base:complaintID a owl:DatatypeProperty ;
    rdfs:domain base:Complaint ;
    rdfs:range xsd:integer .

base:zipCode a owl:DatatypeProperty ;
    rdfs:domain base:Consumer ;
    rdfs:range xsd:integer .
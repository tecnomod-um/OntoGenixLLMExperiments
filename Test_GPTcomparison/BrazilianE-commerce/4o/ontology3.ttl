@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .

base:Customer a owl:Class .

base:CustomerCity a owl:Class ;
    rdfs:subClassOf base:Customer .

base:CustomerState a owl:Class ;
    rdfs:subClassOf base:Customer .

base:CustomerUniqueId a owl:Class ;
    rdfs:subClassOf base:Customer .

base:CustomerZipCodePrefix a owl:Class ;
    rdfs:subClassOf base:Customer .

base:hasCity a owl:ObjectProperty ;
    rdfs:domain base:Customer ;
    rdfs:range base:CustomerCity .

base:hasState a owl:ObjectProperty ;
    rdfs:domain base:Customer ;
    rdfs:range base:CustomerState .

base:hasUniqueId a owl:ObjectProperty ;
    rdfs:domain base:Customer ;
    rdfs:range base:CustomerUniqueId .

base:hasZipCodePrefix a owl:ObjectProperty ;
    rdfs:domain base:Customer ;
    rdfs:range base:CustomerZipCodePrefix .

base:cityName a owl:DatatypeProperty ;
    rdfs:domain base:CustomerCity ;
    rdfs:range rdfs:Literal .

base:stateName a owl:DatatypeProperty ;
    rdfs:domain base:CustomerState ;
    rdfs:range rdfs:Literal .

base:uniqueId a owl:DatatypeProperty ;
    rdfs:domain base:CustomerUniqueId ;
    rdfs:range rdfs:Literal .

base:zipCodePrefix a owl:DatatypeProperty ;
    rdfs:domain base:CustomerZipCodePrefix ;
    rdfs:range rdfs:Literal .

base:customer_city a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range schema:addressLocality .

base:customer_id a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range dcterms:identifier .

base:customer_state a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range schema:addressRegion .

base:customer_unique_id a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range foaf:mbox .

base:customer_zip_code_prefix a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range schema:postalCode .
@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

base:Customer a owl:Class .

base:customer_city a owl:Class ;
    rdfs:subClassOf base:Customer .
base:customer_id a owl:Class ;
    rdfs:subClassOf base:Customer .
base:customer_state a owl:Class ;
    rdfs:subClassOf base:Customer .
base:customer_unique_id a owl:Class ;
    rdfs:subClassOf base:Customer .
base:customer_zip_code_prefix a owl:Class ;
    rdfs:subClassOf base:Customer .

base:hasCity a owl:ObjectProperty ;
    rdfs:domain base:Customer ;
    rdfs:range base:customer_city .
base:hasID a owl:ObjectProperty ;
    rdfs:domain base:Customer ;
    rdfs:range base:customer_id .
base:hasState a owl:ObjectProperty ;
    rdfs:domain base:Customer ;
    rdfs:range base:customer_state .
base:hasUniqueID a owl:ObjectProperty ;
    rdfs:domain base:Customer ;
    rdfs:range base:customer_unique_id .
base:hasZipCodePrefix a owl:ObjectProperty ;
    rdfs:domain base:Customer ;
    rdfs:range base:customer_zip_code_prefix .

base:hasNonNullCount a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .
base:hasType a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:string .
base:hasUniqueCount a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .
base:hasCount a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .
base:hasMax a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .
base:hasMean a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .
base:hasMin a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .
base:hasStd a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .
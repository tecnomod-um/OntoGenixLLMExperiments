@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.geonames.org/> .
@prefix crm: <https://www.w3.org/TR/vocab-crm/> .
@prefix dbpedia: <https://wiki.dbpedia.org/> .
@prefix schema: <https://schema.org/> .
@prefix osm: <https://www.openstreetmap.org/> .

base:City a owl:Class .
base:State a owl:Class .
base:UniqueID a owl:Class .
base:ZipCode a owl:Class .
base:Customer a owl:Class .

base:customer_city a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:string .

base:customer_id a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:string .

base:customer_state a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:string .

base:customer_unique_id a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:string .

base:customer_zip_code_prefix a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:integer .

base:non_null_count a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .

base:type a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:string .

base:unique_count a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .

base:count a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .

base:max a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .

base:mean a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .

base:min a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .

base:std a owl:DatatypeProperty ;
    rdfs:domain base:Customer ;
    rdfs:range xsd:float .

base:customer_city rdfs:seeAlso geo: .
base:customer_id rdfs:seeAlso crm: .
base:customer_state rdfs:seeAlso dbpedia: .
base:customer_unique_id rdfs:seeAlso schema: .
base:customer_zip_code_prefix rdfs:seeAlso osm: .
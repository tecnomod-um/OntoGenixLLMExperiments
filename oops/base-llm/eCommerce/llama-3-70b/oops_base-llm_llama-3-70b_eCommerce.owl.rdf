  <rdf:Description rdf:about="http://oops.linkeddata.es/data/4c20f7cd-f794-4072-bedb-9ff1e06632d0">
    <oops:hasPitfall rdf:resource="http://oops.linkeddata.es/data/e4b729fb-4270-4cd1-8b01-20032812c4aa"/>

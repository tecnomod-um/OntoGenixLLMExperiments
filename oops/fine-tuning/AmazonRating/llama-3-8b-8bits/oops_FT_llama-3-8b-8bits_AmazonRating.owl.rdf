    <rdf:type rdf:resource="http://oops.linkeddata.es/def#pitfall"/>
    <oops:hasAffectedElement rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://vocab.um.es#hasTimestamp</oops:hasAffectedElement>

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://example.com/ontology#> .

:class_entity a owl:Class .

:entity_name rdfs:subClassOf :class_entity .

:hasRating a owl:ObjectProperty ;
    rdfs:domain :class_entity ;
    rdfs:range xsd:integer .

:hasTimestamp a owl:ObjectProperty ;
    rdfs:domain :class_entity ;
    rdfs:range xsd:dateTime .

:hasUserId a owl:ObjectProperty ;
    rdfs:domain :class_entity ;
    rdfs:range xsd:string .

:hasProductId a owl:DatatypeProperty ;
    rdfs:domain :class_entity ;
    rdfs:range xsd:string .

:ProductId rdfs:subClassOf :class_entity ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasProductId ;
        owl:someValuesFrom xsd:string
    ].

:Rating rdfs:subClassOf :class_entity ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasRating ;
        owl:someValuesFrom xsd:integer
    ].

:Timestamp rdfs:subClassOf :class_entity ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasTimestamp ;
        owl:someValuesFrom xsd:dateTime
    ].

:UserId rdfs:subClassOf :class_entity ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasUserId ;
        owl:someValuesFrom xsd:string
    ].
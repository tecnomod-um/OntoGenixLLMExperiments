@prefix base: <https://base_ontology.com#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

base:Product a owl:Class ;
    rdfs:subClassOf schema:Product .

base:Brand a owl:Class ;
    rdfs:subClassOf schema:Brand .

base:Category a owl:Class ;
    rdfs:subClassOf schema:Category .

base:SubCategory a owl:Class ;
    rdfs:subClassOf schema:Category .

base:absoluteUrl a owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf schema:url .

base:imageUrl a owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf schema:image .

base:productName a owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf schema:name .

base:quantity a owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf schema:quantity .

base:discountPrice a owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf schema:price .

base:price a owl:DatatypeProperty ;
    rdfs:domain base:Product ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf schema:price .

base:hasBrand a owl:ObjectProperty ;
    rdfs:domain base:Product ;
    rdfs:range base:Brand .

base:hasCategory a owl:ObjectProperty ;
    rdfs:domain base:Product ;
    rdfs:range base:Category .

base:hasSubCategory a owl:ObjectProperty ;
    rdfs:domain base:Product ;
    rdfs:range base:SubCategory .